<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<%- @entries.each do |entry| -%>
<%- entry.locations.each do |loc| -%>
  <%- unless loc.longitude.nil? -%>
    <Placemark>
      <name><%= h entry.name %></name>
      <description><%= h entry.description %></description>
      <Point>
        <coordinates><%= loc.longitude.to_s + "," + loc.latitude.to_s + ",0" %></coordinates>
      </Point>
    </Placemark>
  <%- end -%>
<%- end -%>
<%- end -%>
</kml>